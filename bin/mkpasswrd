#!/bin/bash

PASSWD_FILE=htpassword
USERS="julich pascal jordan sebastien nicolasm sdelencquesaing smaslo nhaboussi"

for USER in $USERS
do 
	PASSWD=`dd if=/dev/urandom count=128 bs=1 2>&1 | md5sum | cut -b -8`
	htpasswd -m -b $PASSWD_FILE $USER $PASSWD > /dev/null 2> /dev/null
	echo "$USER $PASSWD"
done
